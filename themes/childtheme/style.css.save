 .wp-pagenavi span.current{
		padding:6px 9px 6px 9px !important;
		border:solid 1px #DCDCDC !important;
		border-color:#3390CA !important;
		border-radius:3px !important;	
		-moz-border-radius:3px !important;
		-webkit-border-radius:3px !important;
		color:#fff !important;
		margin-right:3px !important;
		border-color:#202020 !important;
    font-size: 25px;
		background:#525252 !important;
		background:-moz-linear-gradient(top,#9F9F9F 1px,#6C6C6C 1px,#525252) !important;
		background:-webkit-gradient(linear,0 0,0 100%,color-stop(0.02,#9F9F9F),color-stop(0.02,#6C6C6C),color-stop(1,#525252)) !important;
}

.wp-pagenavi .nextpostslink, .wp-pagenavi .previouspostslink{
border:none !important;
}

.arrow-left:before, .arrow-right:before, .nextpostslink:before, .previouspostslink:before {
    top: calc(50% - 0px) !important;
    width: 25px !important;
    height: 2px !important;
    background-color: #000 !important;
}

.arrow-left:after, .arrow-right:after, .nextpostslink:after, .previouspostslink:after {
    top: calc(50% - 5px) !important;
    width: 10px !important;
    height: 10px !important;
    transform: rotate(-45deg);
    border: solid #000 !important;
    border-width: 2px 0 0 2px !important;
}

.arrow-left:before, .arrow-right:before, .nextpostslink:before, .previouspostslink:before, .slick-next:before, .slick-prev:before{
 top: calc(50% - 0px) !important;
    width: 25px !important;
    height: 2px !important;
    background-color: #000 !important;
}

.arrow-left:after, .arrow-right:after, .nextpostslink:after, .previouspostslink:after, .slick-next:after, .slick-prev:after{
top: calc(50% - 5px) !important;
    width: 10px !important;
    height: 10px !important;
    transform: rotate(-45deg);
    border: solid #000 !important;
    border-width: 2px 0 0 2px !important;
}

